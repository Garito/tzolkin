{

  "name": "yTzolkin",
  "version": "1.0.1",

  "description": "Shows the daily kin",

  "permissions": [ "tabs" ],

  "background": {
    "scripts": ["icons.js", "tables.js", "utils.js", "tzolkin.js", "background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "yTzolkin",
    "default_icon": {
      "128": "imgs/semilla.png"
    }
  },

  "icons": {
    "128": "imgs/semilla.png"
  },

  "manifest_version": 2

}